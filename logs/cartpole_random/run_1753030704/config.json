{
  "seed": 42,
  "development_mode": false,
  "packages": {},
  "components": {
    "env": {
      "package": "rltoolbox",
      "type": "SimpleEnvironment",
      "env_name": "CartPole-v1",
      "max_episode_steps": 500
    },
    "agent": {
      "package": "rltoolbox",
      "type": "RandomAgent",
      "action_space_type": "discrete",
      "num_actions": 2
    },
    "console_logger": {
      "package": "rltoolbox",
      "type": "ConsoleLogger",
      "log_frequency": 10,
      "verbose": true,
      "log_steps": false
    },
    "file_logger": {
      "package": "rltoolbox",
      "type": "FileLogger",
      "log_dir": "./logs/cartpole_random",
      "save_frequency": 50,
      "save_config": true
    }
  },
  "hooks": {
    "training_start": [
      "console_logger",
      "file_logger"
    ],
    "episode_start": [],
    "episode_reset": [
      "env"
    ],
    "step_start": [],
    "observation_received": [],
    "action_selection": [
      "agent"
    ],
    "action_chosen": [],
    "environment_step": [
      "env"
    ],
    "transition_received": [],
    "experience_storage": [],
    "learning_update": [],
    "step_end": [],
    "episode_end": [
      "console_logger",
      "file_logger"
    ],
    "training_end": [
      "console_logger",
      "file_logger"
    ]
  },
  "training": {
    "max_episodes": 100,
    "max_steps_per_episode": 500
  },
  "evaluation": {
    "enabled": true,
    "num_episodes": 10,
    "hooks": {
      "evaluation_start": [
        "console_logger",
        "file_logger"
      ],
      "evaluation_episode_end": [
        "console_logger",
        "file_logger"
      ],
      "evaluation_end": [
        "console_logger",
        "file_logger"
      ]
    }
  }
}